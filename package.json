{
  "name": "pair-em-up",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commitmsg": "commitlint --edit $1",
    "precommit": "lint-staged",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "3.6.2",
    "sass": "^1.94.0",
    "vite": "^7.2.2"
  },
  "lint-staged": {
    "*.js": "eslint . --fix",
    "*.{js,scss,html}": "prettier --write"
  }
}
